#include <bits/stdc++.h> 
void rotate(vector<int>& arr, int n) {
    // Write your code here
    int lastElement = arr[n - 1];

    // Shift all elements to the right by one position
    for (int i = n - 1; i > 0; i--) {
        arr[i] = arr[i - 1];
    }

    // Place the last element at the first position
    arr[0] = lastElement;

}
